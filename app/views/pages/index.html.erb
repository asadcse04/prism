
<h1><%=t 'index.welcome'%></h1>

<div class="warn">
  <p><%= t 'index.in_development'%></p>
</div>
<p> <%=t 'index.about_text'%></p>

<ul id="texts">
  <% @documents.each do | document | %>
    <li>
    <div class="doc-tools">
      <%= link_to t(:visualize), visualize_path(document) %>
      <%= link_to t(:highlight), highlight_path(document) %>
      
      <%= link_to document.title, document %>
      <%= document.author %>
    </li>
  <% end %>
</ul>



